{
  "name": "another-ra-api",
  "version": "1.0.0",
  "description": "API server for Another RA application",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "test:preferences": "tsx src/scripts/testPreferences.ts",
    "create:preferences-table": "tsx src/scripts/createPreferencesTable.ts",
    "update:user-preferences-schema": "tsx src/scripts/updateUserPreferencesTableSchema.ts",
    "cleanup:user-preferences-table": "tsx src/scripts/cleanupUserPreferencesTable.ts",
    "fetch:table-ids": "tsx ../scripts/fetch-table-ids.ts",
    "create:integration-marketplace-table": "tsx src/scripts/createIntegrationMarketplaceTable.ts",
    "create:ai-model-registry-table": "tsx src/scripts/createAIModelRegistryTable.ts",
    "add:model-registry-relationship": "tsx src/scripts/addModelRegistryRelationship.ts",
    "add:status-field": "tsx src/scripts/addStatusFieldToTables.ts",
    "check:table-access": "tsx src/scripts/checkTableAccess.ts",
    "add:provider-name-lookup": "tsx src/scripts/addProviderNameLookup.ts",
    "populate:model-registry": "tsx src/scripts/populateModelRegistryFromLocal.ts",
    "test:ai-model-registry": "tsx src/scripts/testAIModelRegistry.ts"
  },
  "dependencies": {
    "@types/multer": "^2.0.0",
    "airtable": "^0.12.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
